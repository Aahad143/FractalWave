QListWidget {
    background-color: #2b2b2b;
    border: none;
    outline: none;
    padding: 5px;
}
QListWidget::item {
    background-color: transparent;
    border-radius: 5px;
    padding: 6px;
    margin: 5px 5px 5px 5px;
    height: 24px;
    width: 24px;
    color: white;
}
QListWidget::item:selected {
    background-color: #3a3a3a;
    color: #ffffff;
}
QListWidget::item:hover {
    background-color: #404040;
}